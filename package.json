{
  "name": "trailblazer-analytics",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "preinstall": "node scripts/maintenance/enforce-pnpm.js",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && node scripts/maintenance/build.js",
    "preview": "astro preview",
    "check": "astro check",
    "lighthouse": "lhci autorun",
    "package:win": "pnpm run build && cd dist && powershell Compress-Archive -Path * -DestinationPath ../trailblazer-analytics-dist.zip -Force && cd ..",
    "package:unix": "pnpm run build && cd dist && zip -r ../trailblazer-analytics-dist.zip . && cd ..",
    "clean": "rmdir /s /q dist 2>nul || rm -rf dist",
    "typecheck": "astro check",
    "fix-warnings": "node scripts/fixes/fix-build-warnings.js",
    "verify-rss": "node scripts/validation/verify-rss.js",
    "consolidate-docs": "node scripts/docs/consolidate-docs.js",
    "organize-docs": "node scripts/docs/organize-docs.js",
    "pre-build": "node scripts/maintenance/pre-build.js",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules",
    "fix:md": "markdownlint \"**/*.md\" --ignore node_modules --fix",
    "validate-content": "node scripts/validation/validate-content.js",
    "tags:build": "astro build && echo 'Tag pages generated successfully'",
    "tags:dev": "astro dev --host 0.0.0.0",
    "content:validate": "node scripts/maintenance/pre-build.js",
    "deploy:preview": "pnpm run build && pnpm run preview",
    "deploy": "pnpm run build",
    "deploy:gh-pages": "touch dist/.nojekyll && gh-pages -d dist -t"
  },  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.2.6",
    "@astrojs/react": "^4.2.7",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/tailwind": "^6.0.2",
    "@headlessui/react": "^1.7.17",
    "@supabase/supabase-js": "^2.39.0",
    "astro": "^5.7.13",
    "chart.js": "^4.4.0",
    "d3": "^7.8.5",
    "framer-motion": "^11.0.3",
    "nanostores": "^0.9.5",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-chartjs-2": "^5.2.0",
    "rss": "^1.2.2",
    "tailwindcss": "^3.4.2",
    "typescript": "^5.8.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@lhci/cli": "^0.14.0",
    "autoprefixer": "10.4.15",
    "gh-pages": "^6.1.1",
    "markdownlint-cli": "^0.45.0",
    "postcss": "8.4.38"
  }
}
