{
  "name": "trailblazer-analytics",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "lighthouse": "lhci autorun",
    "package:win": "pnpm run build && cd dist && powershell Compress-Archive -Path * -DestinationPath ../trailblazer-analytics-dist.zip -Force && cd ..",
    "package:unix": "pnpm run build && cd dist && zip -r ../trailblazer-analytics-dist.zip . && cd ..",
    "clean": "rmdir /s /q dist 2>nul || rm -rf dist",
    "typecheck": "astro check"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.2.6",
    "@astrojs/react": "^4.2.7",
    "@astrojs/rss": "^4.0.12",
    "astro": "^5.7.13",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwindcss": "^3.4.2"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "@lhci/cli": "^0.14.0",
    "autoprefixer": "10.4.15",
    "postcss": "8.4.38"
  }
}